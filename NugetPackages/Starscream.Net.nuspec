<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Starscream.Net</id>
    <version>4.0.6.2</version>
    <title>Xamarin bindings to the native Starscream Sockets SDK.</title>
    <authors>s.bokatuk</authors>
    <owners>s.bokatuk</owners>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <icon>images/icon.png</icon>
    <readme>README.md</readme>
    <projectUrl>https://github.com/sbokatuk/Starscream.Net</projectUrl>
    <description>MAUI Android iOS Mac Xamarin bindings for Starscream Sockets SDK. Alows to use The Starscream Sockets SDK in your app.</description>
    <dependencies>
      <group targetFramework="net7.0-maccatalyst16.1" />
      <group targetFramework="net8.0-maccatalyst17.2" />
      <group targetFramework="net7.0-ios16.1" />
      <group targetFramework="net8.0-ios17.2" />
    </dependencies>
  </metadata>
  <files>
      <file src="sc/Starscream.Net.targets" target="build" />
      <file src="scios/native/lib/ios/**" target="native/lib/ios" />
      <file src="scmac/native/lib/mac/**" target="native/lib/mac" />
      <file src="sc/sdk.png" target="images/icon.png" />
      <file src="sc/README.md" target="" />
      <file src="sc/README.txt" target="" />
      <file src="scios/lib/net7.0-ios16.1/**" target="lib/net7.0-ios16.1" />
	  <file src="scios/lib/net8.0-ios17.2/**" target="lib/net8.0-ios17.2" />
      <file src="scmac/lib/net7.0-maccatalyst16.1/**" target="lib/net7.0-maccatalyst16.1" />
	  <file src="scmac/lib/net8.0-maccatalyst17.2/**" target="lib/net8.0-maccatalyst17.2" />
    </files>
</package>